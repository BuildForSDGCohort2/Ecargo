                        read: {
                            url: '{{url("api/fees")}}',
                        },
                    },
                    pageSize: 10, // display 20 records per page
                    serverPaging: true,
                    serverFiltering: true,
                    serverSorting: true,
                },
                // columns definition
                columns: [
                    {
                        field: 'id',
