    $('body').on('changed.bs.select', '.country_id', function(e, clickedIndex, newValue, oldValue){
        var selected = $(e.currentTarget).val();
        var parent = $(e.currentTarget).parent().parent().parent().parent();
        if(selected != ''){
            {% if addShipmentForm == "add_form_normal" %}
